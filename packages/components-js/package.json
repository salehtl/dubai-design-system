{
  "name": "@dubai-design-system/components-js",
  "version": "0.0.1-alpha.0",
  "description": "Dubai Design System as Web Components",
  "author": "saleh <saleh.lootah@digitaldubai.ae>",
  "keywords": [
    "components",
    "web-components"
  ],
  "homepage": "https://www.npmjs.com/settings/dubai-design-system/packages",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/components-js/components-js.esm.js",
  "exports": {
    ".": {
      "import": "./dist/components-js/components-js.esm.js",
      "require": "./dist/components-js/components-js.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.0",
    "@stencil/react-output-target": "^0.8.1",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
